{
  "name": "@otoseal/injective-agent-kit",
  "version": "0.1.0",
  "description": "Typescript SDK for building AI agents on the Injective",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "test": "tsx test/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "check": "pnpm format && pnpm lint"
  },
  "keywords": [
    "injective",
    "injective-agent-kit"
  ],
  "author": "Otoseal Labs",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/otoseal-labs/injective-agent-kit.git"
  },
  "bugs": {
    "url": "https://github.com/otoseal-labs/injective-agent-kit/issues"
  },
  "homepage": "https://github.com/otoseal-labs/injective-agent-kit#readme",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@langchain/cohere": "^0.3.4",
    "@langchain/langgraph": "^0.4.5",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "dependencies": {
    "@injectivelabs/networks": "^1.16.7",
    "@injectivelabs/sdk-ts": "^1.16.7",
    "@langchain/core": "^0.3.70",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "openai": "^5.12.2",
    "viem": "^2.33.3",
    "zod": "<=3.25.67"
  }
}
